Host github.com-creativecreature
HostName github.com
AddKeysToAgent yes
User git
IdentityFile ~/.ssh/creativecreature_rsa
IdentitiesOnly yes

Host localhost
HostName 127.0.0.1
Port 22
User pair
IdentityFile ~/.ssh/private_rsa
StrictHostKeyChecking no

Host stash.int.klarna.net
AddKeysToAgent yes
User git
IdentityFile ~/.ssh/klarna_rsa
IdentitiesOnly yes
